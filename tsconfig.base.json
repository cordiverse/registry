{
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",
    "composite": true,
    "emitDeclarationOnly": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "nodenext",
    "strictBindCallApply": true,
    "allowImportingTsExtensions": true,
  },
}
